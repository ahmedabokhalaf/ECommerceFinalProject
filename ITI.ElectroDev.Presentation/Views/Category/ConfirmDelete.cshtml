
<div class="col-10 mx-auto mt-5">
    <div class="card shadow border-0">
        <div class="card-body">
            <div class="row">
                <h1>Confirm Delete</h1>
                <h1>Are You Sure To Delete This Category :@Model.Name</h1>
                <a class="col-1 btn  btn-danger" asp-controller="Category" asp-action="Delete" asp-route-id="@Model.Id">Yes</a>
                <a class="col-1 btn btn-primary" asp-controller="Category" asp-action="Index">Cancel</a>
            </div>
        </div>
    </div>
</div>

